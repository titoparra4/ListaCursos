@page
@model ListaCursos.Pages.CoursesModel
@{
    ViewData["Title"] = "Courses";
}

<h2>Courses</h2>

<table>
    @foreach (var course in Model.Courses)
    {
    <tr>
        <td>@course.Id</td>
        <td>@course.Name</td>
        <td>@course.Author</td>
        <td>@course.Description</td>
        <td>@course.Uri</td>
    </tr>
    }
</table>

